#!/bin/csh -f

if ($?SRC == 0) then
    echo $1": Sorry, you must set up your environment first.  Assuming"
    echo $1": you at least have "'$'"HOPS defined, type the following"
    echo $1": command ... 'source "'$'"HOPS/setup.csh' and try again."
    echo $1": Otherwise, see your local HOPS guru"
    exit (1)
else
    exit (0)
endif
